{
    "name": "tonis-io/di",
    "authors": [
        {
            "name": "Kyle Spraggs",
            "email": "theman@spiffyjr.me"
        }
    ],
    "description": "A light-weight, HHVM compatible, and dependency lite DI container.",
    "license": "BSD-3-Clause",
    "require": {
        "php": ">=5.5",
        "container-interop/container-interop": "^1.1"
    },
    "require-dev": {
        "doctrine/annotations": "^1.2",
        "phpunit/phpunit": "^4.0",
        "squizlabs/php_codesniffer": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Tonis\\Di\\": [
                "src/"
            ]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tonis\\Di\\": [
                "test/"
            ]
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
